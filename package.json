{
  "name": "gatsby-typescript",
  "version": "2.0.0",
  "description": "Alternative typescript support plugin for Gatsbyjs. Aims to make using typescript in Gatsby as painless as possible",
  "main": "index.js",
  "repository": "https://github.com/d4rekanguok/gatsby-plugin-ts.git",
  "author": "Derek Nguyen <derek@penandpillow.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "build": "lerna run build",
    "test": "jest",
    "confirm": "node scripts/confirm",
    "lint": "eslint ."
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/gatsby-transformer-sharp",
      "**/gatsby-transformer-remark/**"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/jest": "25.2.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "graphql": "14.7.0",
    "jest": "25.5.4",
    "lerna": "3.22.1",
    "prettier": "1.19.1"
  }
}
